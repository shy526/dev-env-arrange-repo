{
  "name": "scala3",
  "check": "scala -version",
  "variable":{
    "owner":"lampepfl",
    "repo":"dotty"
  },
  "download": {
    "urlRoot": [
      "/"
    ],
    "process": "githubProcess",
    "url": "${urlRoot}/${version}/scala3-${version}.${os-format}"
  },
  "operate": [
    "env SCALA_HOME_SHY ${root} + PATH %SCALA_HOME_SHY%\\bin += < "
  ]
}
